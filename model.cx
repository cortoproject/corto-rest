in package /corto/rest
use corto/httpserver

// REST interface that provides access to Corto store
class service: httpserver/Service::
    from: string
    int16 construct()
    int16 on_get(HTTP/Connection c, HTTP/Request r, string uri) override
    int16 on_post(HTTP/Connection c, HTTP/Request r, string uri) override
    int16 on_put(HTTP/Connection c, HTTP/Request r, string uri) override
    int16 on_delete(HTTP/Connection c, HTTP/Request r, string uri) override
